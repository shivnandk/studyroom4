{
    "builds": [
      {
        "src": "studybud/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles/$1",
        "headers": {
          "Cache-Control": "max-age=31536000"   
        }
      },
      {
        "src": "/(.*)",
        "dest": "studybud/wsgi.py"
      }
    ]
  }
  